{
    "manifest_version": 3,
    "name": "Tabless",
    "version": "1.0",
    "description": "Store and organize your Chrome tabs",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png"
          }
    },
    "host_permissions": [
        "https://developer.chrome.com/*"
    ],
    "permissions": [
        "tabGroups",
        "tabs",
        "storage",
        "sessions",
        "tabGroups"
    ],
    "browser_action": {
        "default_icon":"./icons-16.png",
        "default_popup":"popup.html"
    }
}
      
    
